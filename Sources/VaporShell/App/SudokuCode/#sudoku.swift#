class s{
    public var row : Int
    public var column : Int
    public var box : Int
    public var cellIndex: Int
    public var number = 0

    init(r: Int, c: Int, b: Int, i: Int) {
        self.row = r
        self.column = c
        self.box = b
        self.cellIndex = i
    }
}

